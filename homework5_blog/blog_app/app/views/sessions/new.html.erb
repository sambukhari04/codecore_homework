
  <h1>Sign In</h1>

<div class="row">
  <div class="col-md-6">

    <%= form_tag session_path do %>
    <div>
      <%= label_tag :email %>
      <%= email_field_tag :email, "", class:'form-control' %>
    </div>
    <div>
      <%= label_tag :password %>
      <%= password_field_tag :password, "", class:'form-control'%>
    </div>
    <div>
      <%= submit_tag "Log In", class: "btn btn-primary"  %>
    </div>
    <% end %>
  </div>
</div>
